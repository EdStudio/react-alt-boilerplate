{
  "name": "react-alt-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for Isomorphic Application using ReactJS and alt.js with webpack and babel",
  "main": "main.jsx",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server",
    "lint": "eslint app",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edstudio/react-alt-boilerplate.git"
  },
  "author": "Edmund Hung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edstudio/react-alt-boilerplate/issues"
  },
  "homepage": "https://github.com/edstudio/react-alt-boilerplate",
  "dependencies": {
    "alt": "^0.16.10",
    "iso": "^4.1.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "react-tools": "^0.13.3"
  },
  "devDependencies": {
    "babel-core": "^5.6.15",
    "babel-eslint": "^3.1.19",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.2.2",
    "babel-plugin-object-assign": "^1.2.0",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.6.2",
    "jest-cli": "^0.4.14",
    "node-libs-browser": "^0.5.2",
    "react-hot-loader": "^1.2.7",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ]
  }
}
